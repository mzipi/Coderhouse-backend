<h1>{{titulo}}</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>TITLE</th>
            <th>PRICE</th>
            <th>THUMBNAIL</th>
        </tr>
    </thead>
    <tbody>
        {{#each mensaje}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src="{{this.thumbnail}}" alt="{{this.title}}"></td>
        </tr>
        {{/each}}
    </tbody>
</table>